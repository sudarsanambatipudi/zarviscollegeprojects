<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
  "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.lms.domain.sub.Bal" table="bal" discriminator-value="0">
        <id column="ID" name="id" type="java.lang.Integer">
            <generator class="native" />
        </id>
        <property column="GUID" name="guid" type="java.lang.String" />

        <property column="EMPLOYEEID" name="employeeId" type="java.lang.String" />        
        <property column="TOTAL" name="total" type="java.lang.String" />
        <property column="CREATEDON" name="createdOn" type="java.util.Date" />
        


    </class>
</hibernate-mapping>