<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.dmsapp.domain.sub.Attendance" table="attendance_master" discriminator-value="0">
        <id column="id" name="id" type="java.lang.Integer">
            <generator class="native" />
        </id>

        <property column="year" name="year" type="java.lang.String" />
        <property column="branch" name="branch" type="java.lang.String" />
        <property column="semister" name="semister" type="java.lang.String" />
        <property column="paper" name="paper" type="java.lang.String" />
        <property column="papercode" name="paperCode" type="java.lang.String" />
        <property column="numberOfWorkingDays" name="numberOfWorkingDays" type="java.lang.String" />
        <property column="active" name="active" type="java.lang.Integer" />

    </class>
</hibernate-mapping>
